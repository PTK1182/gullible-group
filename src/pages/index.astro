---

<Layout>
    
<div class="bg-white overflow-x-clip py-10">
    <div class="">
      <div class="">
        <div class="flex justify-center">
          <img src="/hero.svg" fetchpriority="high" class="max-w-[500px] w-[300px] h-auto sm:w-[500px] sm:h-auto" width="500" height="auto" />
        </div>
        <div class="w-full flex justify-center ">
          <img src="/hero_subtitle.svg" class="max-w-[500px] w-[300px] sm:w-[500px]" width="500" height="auto" />
        </div>
      </div>
    </div>
    <Carousel />
    <Voices />
  </div>
</Layout>

